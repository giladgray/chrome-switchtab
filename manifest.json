{
  "name": "SwitchTab",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Quickly jump between open tabs across all windows!",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://code.jquery.com/",
    "tabs"
  ]
}